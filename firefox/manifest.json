{
	"name": "Whatsapp Web Dark",
	"version": "1.1",
	"description": "Click the icon to enable Whatsapp Web dark mode for PC. Bugs are Whatsapp, not me.",
	"author": "luismzk",
  "homepage_url": "https://github.com/luismzk/whatsappweb-dark",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["*://web.whatsapp.com/*"],
      "js": ["wsScript.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "storage", "activeTab", "declarativeContent", "tabs"
  ],
  "browser_action": {
    "default_title": "WS Web Dark mode",
    "default_popup": "popup.html"
  },
	"manifest_version": 2
}